jobs:
  example-job:
    runs-on: ubuntu-latest
    steps:
      - name: Checkout repository
        uses: actions/checkout@v2
      - name: Use Google API
        run: |
          echo "Using Google API..."
          # 예를 들어, curl을 사용하여 Google API와 상호 작용하는 스크립트
          curl -H "Authorization: Bearer ${{ secrets.GSC_API_KEY }}" https://example.com/api
